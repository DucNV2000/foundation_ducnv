}

.tag-background-edit {
    background-image: url('project://database/Assets/heart/Modules/Tag/Editor/tag_btn_edit.png?fileID=2800000&guid=63b01688b61ee7c439e8008bbe8981c5&type=3#tag_btn_edit');
}

.tag-background-duplicate {
    background-image: url('project://database/Assets/heart/Modules/Tag/Editor/tag_btn_duplicate.png?fileID=2800000&guid=5e49cf2cdb83d004ebcd403b5ab35296&type=3#tag_btn_duplicate');
}

.tag-background-delete {
    background-image: url('project://database/Assets/heart/Modules/Tag/Editor/tag_btn_delete.png?fileID=2800000&guid=885ac94bef3a2754a8d5aa318ac66269&type=3#tag_btn_delete');
}

.tag-query-tag-region {
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
    border-top-width: 1px;
    border-left-color: rgb(84, 84, 84);
    border-right-color: rgb(84, 84, 84);
    border-top-color: rgb(84, 84, 84);
    border-bottom-color: rgb(84, 84, 84);
    padding-left: 4px;
    padding-right: 4px;
    padding-top: 4px;
    padding-bottom: 4px;
}

.tag-query-tag-region > .unity-label {
    opacity: 0.7;
}

.tag-simple {
    flex-direction: row;
    background-color: rgb(82, 82, 82);
    border-top-left-radius: 12px;
    border-bottom-left-radius: 12px;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    margin-left: 3px;
    margin-right: 3px;
    margin-top: 3px;
    margin-bottom: 3px;
    align-items: center;
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    max-width: 200px;
    justify-content: space-between;
    border-left-color: rgba(0, 0, 0, 0);
    border-right-color: rgba(0, 0, 0, 0);
    border-top-color: rgba(0, 0, 0, 0);
    border-bottom-color: rgba(0, 0, 0, 0);
}

.tag-simple:hover {
    border-bottom-width: 1px;
    border-right-width: 1px;
    border-left-color: rgba(255, 255, 255, 0.49);
    border-right-color: rgba(255, 255, 255, 0.49);
    border-top-color: rgba(255, 255, 255, 0.49);
    border-bottom-color: rgba(255, 255, 255, 0.49);
    border-left-width: 1px;
    border-top-width: 1px;
}

.tag-rule-include > .tag-simple {
    background-color: rgb(67, 57, 221);
}

.tag-rule-any > .tag-simple {
    background-color: rgb(10, 115, 10);
}

.tag-rule-exclude > .tag-simple {
    background-color: rgb(118, 23, 29);
}

.tag-drop-valid {
    border-bottom-width: 1px;
    border-right-width: 1px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 1px;
    border-top-width: 1px;
}

.tag-drop-valid:hover {
    border-bottom-width: 1px;
    border-right-width: 1px;
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 1px;
    border-top-width: 1px;
}

.tag-simple-delete {
    background-color: rgba(0, 0, 0, 0);
    border-top-left-radius: 0;
    border-bottom-left-radius: 0;
    border-top-right-radius: 12px;
    border-bottom-right-radius: 12px;
    -unity-background-image-tint-color: rgba(255, 255, 255, 0.61);
    border-left-width: 1px;
    border-left-color: rgba(183, 183, 183, 0.21);
    border-right-color: rgba(183, 183, 183, 0);
    border-top-color: rgba(183, 183, 183, 0);
    border-bottom-color: rgba(183, 183, 183, 0);
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    margin-left: 0;
    margin-right: 0;
    margin-top: 0;
    margin-bottom: 0;
}

.tag-simple-delete:hover {
    border-left-color: rgb(255, 255, 255);
    border-right-color: rgb(255, 255, 255);
    border-top-color: rgb(255, 255, 255);
    border-bottom-color: rgb(255, 255, 255);
    border-left-width: 1px;
    border-right-width: 1px;
    border-top-width: 1px;
    border-bottom-width: 1px;
    -unity-background-image-tint-color: rgb(255, 255, 255);
    background-color: rgba(118, 118, 118, 0.45);
}
